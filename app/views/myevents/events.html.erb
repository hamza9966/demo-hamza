
<div class="container">
    <h1>Events Listing</h1>
    <% @event_list.items.each do |event| %>
    <div class="container_in">
      <span class=""><%= 'Title' %>:  <%= event.summary %></span>
      <p>Description : <%= event.description%></p>
      <ul>
        <li>Start : <%= event.start.date %></li>
        <li>end : <%= event.end.date%></li>
      </ul>
    </div>
    <%end %>
</div>