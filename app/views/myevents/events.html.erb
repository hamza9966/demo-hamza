<div class="row">
  <div class="col s12">
    <h3 class="center-align">Events Listing</h3>
  </div>
</div>

<div class="row">
  <% @event_list.items.each do |event| %>
    <div class="col s6 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= 'Title' %>:  <%= event.summary %></span>
          <li>Description : <%= event.description%></li>
          <li>Start : <%= event.start.date %>
          <li>end : <%= event.end.date%>
        </div>
        <div class="card-action">
        </div>

      </div>
    </div>
  <%end %>
</div>